{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name":"client",
			"image": "shdkapoor/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name":"server",
			"image": "shdkapoor/multi-server",
			"hostname": "api",
			"essential": false
		},
		{
			"name":"worker",
			"image": "shdkapoor/multi-worker",
			"essential": false
		},
		{
			"name":"nginx",
			"image": "shdkapoor/multi-nginx",
			"essential": true,
			"portMappings": [
				{
					"hostport": 80,
					"containerport": 80
				}
			],
			"links": ["client", "server"]
		}
	]
}